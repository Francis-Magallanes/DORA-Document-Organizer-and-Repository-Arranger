<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddFilesController">
   <children>
      <TextField fx:id="FileField" editable="false" layoutX="103.0" layoutY="133.0" prefHeight="25.0" prefWidth="334.0" />
      <Label layoutX="72.0" layoutY="137.0" text="File:" />
      <Button fx:id="Browse" layoutX="453.0" layoutY="133.0" mnemonicParsing="false" onAction="#browseFile" text="Browse" />
      <TextField fx:id="NameField" layoutX="173.0" layoutY="360.0" prefHeight="25.0" prefWidth="218.0" />
      <Label layoutX="123.0" layoutY="364.0" text="Name:" />
      <TextField fx:id="YearField" layoutX="173.0" layoutY="413.0" prefHeight="25.0" prefWidth="218.0" />
      <Label layoutX="72.0" layoutY="417.0" text="Year Graduated:" />
      <Button fx:id="AddFile" layoutX="205.0" layoutY="488.0" mnemonicParsing="false" onAction="#AddFileToDatabase" prefHeight="58.0" prefWidth="130.0" text="Add File" />
      <Button fx:id="Back" layoutX="429.0" layoutY="488.0" mnemonicParsing="false" onAction="#back" prefHeight="58.0" prefWidth="130.0" text="Back" />
   </children>
</AnchorPane>
