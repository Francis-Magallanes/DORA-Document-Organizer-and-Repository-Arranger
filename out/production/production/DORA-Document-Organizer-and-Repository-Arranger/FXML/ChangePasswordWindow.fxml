<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" stylesheets="@../assets/bootstrap3.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ChangePasswordController">
   <children>
      <Label layoutX="197.0" layoutY="14.0" text="Change Password">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label layoutX="101.0" layoutY="79.0" prefHeight="32.0" prefWidth="100.0" text="Old Password:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="97.0" layoutY="128.0" prefHeight="30.0" prefWidth="100.0" text="New Password:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="23.0" layoutY="179.0" prefHeight="30.0" prefWidth="178.0" text="Re-type the New Password:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <PasswordField fx:id="oldPasswordField" layoutX="214.0" layoutY="80.0" prefHeight="20.0" prefWidth="317.0" />
      <PasswordField fx:id="newPasswordField" layoutX="214.0" layoutY="128.0" prefHeight="20.0" prefWidth="317.0" />
      <PasswordField fx:id="retypePassword" layoutX="214.0" layoutY="177.0" prefHeight="20.0" prefWidth="317.0" />
      <Button layoutX="124.0" layoutY="237.0" mnemonicParsing="false" onAction="#ChangePasswordAction" prefHeight="32.0" prefWidth="137.0" text="Change Password" />
      <Button layoutX="340.0" layoutY="237.0" mnemonicParsing="false" onAction="#cancel" prefHeight="42.0" prefWidth="129.0" text="Cancel" />
   </children>
</AnchorPane>
