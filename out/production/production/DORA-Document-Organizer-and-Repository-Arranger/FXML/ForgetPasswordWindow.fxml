         <font>
            <Font size="14.0" />
         </font></Label>
      <Button fx:id="Submit" layoutX="321.0" layoutY="196.0" mnemonicParsing="false" onAction="#Check" prefHeight="25.0" prefWidth="94.0" text="Submit" />
      <Button layoutX="152.0" layoutY="196.0" mnemonicParsing="false" onAction="#Back" prefHeight="25.0" prefWidth="94.0" text="Back" />
